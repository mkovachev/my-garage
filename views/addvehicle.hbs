<div>
    <form method="post" action="/addvehicle">
        <div class="form-group">
            <label>Car Type</label>
            <input type="text" class="form-control" placeholder="Car Type" name="type">
        </div>
        <div class="form-group">
            <label>Brand</label>
            <input type="text" class="form-control" placeholder="Brand" name="brand">
        </div>
        <div class="form-group">
            <label>Model</label>
            <input type="text" class="form-control" placeholder="Model" name="model">
        </div>
        <div class="form-group">
            <label>License</label>
            <input type="text" class="form-control" placeholder="License" name="license">
        </div>
        <div class="form-group">
            <label>Year</label>
            <input type="text" class="form-control" placeholder="Year" name="year">
        </div>
        <div class="form-group">
            <label>Km</label>
            <input type="text " class="form-control" placeholder="Km" name="km">
        </div>
        <button type="submit" class="btn btn-default">Submit</button>
    </form>
</div>